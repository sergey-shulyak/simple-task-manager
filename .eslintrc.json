{
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "rules": {
    "indent": [
      "error",
      2
    ],
    "comma-dangle": [
      "error",
      "never"
    ],
    "object-curly-newline": [
      "error",
      {
        "ObjectExpression": {
          "minProperties": 5,
          "consistent": true
        },
        "ObjectPattern": {
          "minProperties": 6,
          "consistent": true
        }
      }
    ],
    "no-underscore-dangle": 0,
    "no-console": 0,
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "react/jsx-indent": [
      "enabled",
      2
    ],
    "jsx-a11y/anchor-is-valid": 0
  }
}